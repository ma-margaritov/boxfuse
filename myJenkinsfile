pipeline
{
  agent { 
         dockerfile 
         {
          filename 'myDockerfile'
         }
        }
    stages
  {
    stage('Copy source')
    {
      steps { git 'https://github.com/ma-margaritov/boxfuse.git' }
    }
    stage('Build war')
    {
      steps { sh 'mvn package' }
    }
    stage('Make docker image')
        {
      steps { 
            //  sh 'docker build -t boxfuse .' 
                sh 'echo docker-build-stage'    
            }
          // && docker push'
    }
   }
}
